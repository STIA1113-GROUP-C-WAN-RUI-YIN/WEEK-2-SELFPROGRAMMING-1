package QuickCheck;

public class printf {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        double price = 1.21997;
		System.out.printf("%.2f", price);
		

	
	}

}
package QuickCheck;

public class printf2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double price = 1.21997;
		System.out.printf("%10.2f", price);
	}

}
package QuickCheck;

public class printf3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x= 10; 
		double y = 5.373123;
		System.out.printf("Value of x is %d and value of y is %6.2f  %n", x , y);

	}

}
package QuickCheck;

public class printf4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    double price = 17.2876;
    System.out.printf("%-10s%10.2f","Total : ",price);
	}

}
